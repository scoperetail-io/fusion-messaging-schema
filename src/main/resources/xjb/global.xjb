<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<jaxb:bindings
        xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" xmlns:xs="http://www.w3.org/2001/XMLSchema"
        xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/jaxb http://java.sun.com/xml/ns/jaxb/bindingschema_2_0.xsd"
        version="2.1">

    <jaxb:globalBindings typesafeEnumMaxMembers="600" typesafeEnumMemberName="generateError">
        <xjc:javaType adapter="com.scoperetail.fusion.messaging.schema.xjb.adapter.LocalDateTimeAdapter" name="java.time.LocalDateTime" xmlType="xs:dateTime"/>
        <xjc:javaType adapter="com.scoperetail.fusion.messaging.schema.xjb.adapter.LocalDateAdapter" name="java.time.LocalDate" xmlType="xs:date"/>
        <xjc:javaType adapter="com.scoperetail.fusion.messaging.schema.xjb.adapter.LocalTimeAdapter" name="java.time.LocalTime" xmlType="xs:time"/>
    </jaxb:globalBindings>

</jaxb:bindings>